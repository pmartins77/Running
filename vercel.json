{
  "builds": [{ "src": "index.html", "use": "@vercel/static" }],
  "routes": [
    { "src": "/script.js", "dest": "/script.js" },
    { "src": "/app.js", "dest": "/app.js" },
    { "src": "/styles.css", "dest": "/styles.css" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" },
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}